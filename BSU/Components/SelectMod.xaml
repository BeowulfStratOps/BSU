<UserControl x:Class="BSU.GUI.Components.SelectMod"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:viewModel="clr-namespace:BSU.Core.ViewModel;assembly=BSU.Core"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300" d:DataContext="{d:DesignInstance viewModel:SelectMod}">
    <TextBlock Padding="10,0,0,0">
        <Run Text="{Binding Path=ActionType, Mode=OneWay}"> </Run> <Run Text="{Binding Path=Name, Mode=OneWay}"></Run>
    </TextBlock>
</UserControl>

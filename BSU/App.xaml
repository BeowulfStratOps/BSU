<Application x:Class="BSU.GUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:gui="clr-namespace:BSU.GUI">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Skin.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Svgs.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/MetroWindow.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Scrollbar.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Button.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/RadioToggleButton.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/CheckBox.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/ComboBox.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/MenuItem.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Expander.xaml"></ResourceDictionary>
                <gui:SkinFile />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
